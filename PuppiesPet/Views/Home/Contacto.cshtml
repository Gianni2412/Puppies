                                                    <input asp-for="NombreMascota" type="text" class="form-control"
                                                        id="exampleFormControlInput1" placeholder="Ej:Koki">
                                                </div>
                                                <div>
                                                    <label asp-for="CorreoElectronico" for="exampleFormControlInput1"
                                                        class="form-label">Correo
                                                        Electr칩nico:</label>
                                                    <input asp-for="CorreoElectronico" type="email" class="form-control"
                                                        id="exampleFormControlInput1" placeholder="luis@example.com">
                                                </div>
                                                <div>
                                                    <label asp-for="Asunto" for="exampleFormControlTextarea1"
                                                        class="form-label">Asunto:</label>
                                                    <input asp-for="Asunto" class="form-control"
                                                        id="exampleFormControlTextarea1" rows="3"></input>
                                                </div>
                                                <div class="form-group enviar pt-3 pb-3 text-center ">
                                                    <button type="submit" class="btn">Enviar</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--js-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"
